  <DataSets>
    <DataSet Name="Placanje">
      <Query>
        <DataSourceName>onlineShop</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@NacinPlacanjaID">
            <Value>=Parameters!NacinPlacanjaID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select k.ImePrezime as 'Ime prezime', P.Iznos as 'Iznos',
CONVERT(nvarchar, P.Datum,104) as 'Datum uplate', 
P.NacinPlacanjaID ,NP.Opis as 'Nacin placanja'
from Kupac as K join Narudzba as N
on K.KupacID = N.KupacID join Placanje as P
on P.NarudzbaId = N.NarudzbaID join NacinPlacanja as NP
on NP.NacinPlacanjaID = P.NacinPlacanjaID
where P.NacinPlacanjaID = @NacinPlacanjaID OR @NacinPlacanjaID IS NULL </CommandText>
      </Query>
      <Fields>
        <Field Name="Ime_prezime">
          <DataField>Ime prezime</DataField>
